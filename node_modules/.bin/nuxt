#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/Palette-Generator/Palette-Generator/node_modules/.pnpm/nuxt@4.1.2_@parcel+watcher@2.5.1_@types+node@24.6.2_@vue+compiler-sfc@3.5.22_db0@0.3.4__fb415d24e2b4d20038a72527007148cd/node_modules/nuxt/bin/node_modules:/home/runner/work/Palette-Generator/Palette-Generator/node_modules/.pnpm/nuxt@4.1.2_@parcel+watcher@2.5.1_@types+node@24.6.2_@vue+compiler-sfc@3.5.22_db0@0.3.4__fb415d24e2b4d20038a72527007148cd/node_modules/nuxt/node_modules:/home/runner/work/Palette-Generator/Palette-Generator/node_modules/.pnpm/nuxt@4.1.2_@parcel+watcher@2.5.1_@types+node@24.6.2_@vue+compiler-sfc@3.5.22_db0@0.3.4__fb415d24e2b4d20038a72527007148cd/node_modules:/home/runner/work/Palette-Generator/Palette-Generator/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/Palette-Generator/Palette-Generator/node_modules/.pnpm/nuxt@4.1.2_@parcel+watcher@2.5.1_@types+node@24.6.2_@vue+compiler-sfc@3.5.22_db0@0.3.4__fb415d24e2b4d20038a72527007148cd/node_modules/nuxt/bin/node_modules:/home/runner/work/Palette-Generator/Palette-Generator/node_modules/.pnpm/nuxt@4.1.2_@parcel+watcher@2.5.1_@types+node@24.6.2_@vue+compiler-sfc@3.5.22_db0@0.3.4__fb415d24e2b4d20038a72527007148cd/node_modules/nuxt/node_modules:/home/runner/work/Palette-Generator/Palette-Generator/node_modules/.pnpm/nuxt@4.1.2_@parcel+watcher@2.5.1_@types+node@24.6.2_@vue+compiler-sfc@3.5.22_db0@0.3.4__fb415d24e2b4d20038a72527007148cd/node_modules:/home/runner/work/Palette-Generator/Palette-Generator/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
fi
