#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/Palette-Generator/Palette-Generator/node_modules/.pnpm/@nuxt+ui@4.0.0_@babel+parser@7.28.4_change-case@5.4.4_db0@0.3.4_embla-carousel@8.6.0_io_39459b2a2c0ccb4a3dcbc5c90926b18f/node_modules/@nuxt/ui/cli/node_modules:/home/runner/work/Palette-Generator/Palette-Generator/node_modules/.pnpm/@nuxt+ui@4.0.0_@babel+parser@7.28.4_change-case@5.4.4_db0@0.3.4_embla-carousel@8.6.0_io_39459b2a2c0ccb4a3dcbc5c90926b18f/node_modules/@nuxt/ui/node_modules:/home/runner/work/Palette-Generator/Palette-Generator/node_modules/.pnpm/@nuxt+ui@4.0.0_@babel+parser@7.28.4_change-case@5.4.4_db0@0.3.4_embla-carousel@8.6.0_io_39459b2a2c0ccb4a3dcbc5c90926b18f/node_modules/@nuxt/node_modules:/home/runner/work/Palette-Generator/Palette-Generator/node_modules/.pnpm/@nuxt+ui@4.0.0_@babel+parser@7.28.4_change-case@5.4.4_db0@0.3.4_embla-carousel@8.6.0_io_39459b2a2c0ccb4a3dcbc5c90926b18f/node_modules:/home/runner/work/Palette-Generator/Palette-Generator/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/Palette-Generator/Palette-Generator/node_modules/.pnpm/@nuxt+ui@4.0.0_@babel+parser@7.28.4_change-case@5.4.4_db0@0.3.4_embla-carousel@8.6.0_io_39459b2a2c0ccb4a3dcbc5c90926b18f/node_modules/@nuxt/ui/cli/node_modules:/home/runner/work/Palette-Generator/Palette-Generator/node_modules/.pnpm/@nuxt+ui@4.0.0_@babel+parser@7.28.4_change-case@5.4.4_db0@0.3.4_embla-carousel@8.6.0_io_39459b2a2c0ccb4a3dcbc5c90926b18f/node_modules/@nuxt/ui/node_modules:/home/runner/work/Palette-Generator/Palette-Generator/node_modules/.pnpm/@nuxt+ui@4.0.0_@babel+parser@7.28.4_change-case@5.4.4_db0@0.3.4_embla-carousel@8.6.0_io_39459b2a2c0ccb4a3dcbc5c90926b18f/node_modules/@nuxt/node_modules:/home/runner/work/Palette-Generator/Palette-Generator/node_modules/.pnpm/@nuxt+ui@4.0.0_@babel+parser@7.28.4_change-case@5.4.4_db0@0.3.4_embla-carousel@8.6.0_io_39459b2a2c0ccb4a3dcbc5c90926b18f/node_modules:/home/runner/work/Palette-Generator/Palette-Generator/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@nuxt/ui/cli/index.mjs" "$@"
else
  exec node  "$basedir/../@nuxt/ui/cli/index.mjs" "$@"
fi
