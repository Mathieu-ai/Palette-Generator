{
    "name": "palette-generator",
    "description": "A tool for generating color palettes",
    "type": "module",
    "private": true,
    "packageManager": "pnpm@10.14.0",
    "scripts": {
        "build": "nuxt build",
        "dev": "nuxt dev",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "lint:locales": "pnpm exec prettier --write \"i18n/locales/**/*.json\"",
        "deploy:local": "NUXT_APP_BASE_URL=/Palette-Generator/ pnpm generate && npx serve .output/public",
        "clean": "rm -rf .output .nuxt node_modules/.cache"
    },
    "dependencies": {
        "@googlemaps/markerclusterer": "^2.6.2",
        "@nuxt/eslint": "1.9.0",
        "@nuxt/image": "1.11.0",
        "@nuxt/scripts": "0.12.1",
        "@nuxt/ui": "4.0.0",
        "@nuxtjs/i18n": "10.1.0",
        "@pinia/nuxt": "0.11.2",
        "@unhead/vue": "^2.0.17",
        "colorthief": "^2.6.0",
        "eslint": "^9.36.0",
        "nuxt": "^4.1.2",
        "tailwindcss": "^4.1.14",
        "typescript": "^5.9.3",
        "vue": "^3.5.22",
        "vue-router": "^4.5.1"
    },
    "devDependencies": {
        "@iconify-json/heroicons": "^1.2.3",
        "@iconify-json/lucide": "^1.2.68",
        "@types/colorthief": "^2.6.0",
        "@types/node": "^24.6.2",
        "prettier": "^3.6.2"
    }
}
